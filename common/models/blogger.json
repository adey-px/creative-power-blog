{
  "name": "Blogger",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "profile": {
      "type": "hasOne",
      "model": "Profile",
      "foreignKey": "userId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AuthToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "blogPost": {
      "type": "hasMany",
      "model": "BlogPost",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
