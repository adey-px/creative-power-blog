{
    "name": "BlogPost",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "title": {
            "type": "string"
        },
        "slug": {
            "type": "string"
        },
        "content": {
            "type": "string"
        },
        "createdAt": {
            "type": "date"
        }
    },
    "validations": [],
    "relations": {
        "blogger": {
            "type": "belongsTo",
            "model": "Blogger",
            "foreignKey": "userId"
        },
        "profile": {
            "type": "belongsTo",
            "model": "Profile",
            "foreignKey": "profileId"
        }
    },
    "acls": [],
    "methods": {}
}